<h1>My Playlists</h1>
<ul>
    {{#each playlists}}
        <li>
            <h2>{{this.name}}</h2>
            <p>{{this.description}}</p>
            <img src="{{this.images.[0].url}}" alt="{{this.name}}" width="200">
            <a href="/export-pdf/{{this.id}}">Export PDF</a>

        </li>
    {{/each}}
</ul>